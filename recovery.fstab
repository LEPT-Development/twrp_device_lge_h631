/boot		   emmc		/dev/block/mmcblk0p18
/system		   ext4		/dev/block/mmcblk0p36
/data		   ext4		/dev/block/mmcblk0p38
/cache		   ext4		/dev/block/mmcblk0p37
/recovery	   emmc		/dev/block/mmcblk0p19
/efs1		   emmc         /dev/block/mmcblk0p15  flags=backup=1;display=EFS
/efs2		   emmc         /dev/block/mmcblk0p16  flags=backup=1;subpartitionof=/efs1

/external_sd	   vfat         /dev/block/mmcblk1p1   /dev/block/mmcblk1
